	<Menu>
		<Name>System</Name>
		<AppDir>/home/adam/.local/share/applications</AppDir>
		<Exclude>
			<Filename>gcm-picker.desktop</Filename>
		</Exclude>
		<Exclude>
			<Filename>org.freedesktop.MalcontentControl.desktop</Filename>
		</Exclude>
